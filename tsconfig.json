{
  "compilerOptions": {
    "alwaysStrict": true,
    "charset": "utf8",
    "declaration": true,
    "experimentalDecorators": true,
    "lib": ["es2018"],
    "module": "CommonJS",
    "noEmitOnError": true,
    "resolveJsonModule": true,
    "strict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "stripInternal": true,
    "target": "ES2018",
    "incremental": true,
    "skipLibCheck": true
  },
  "include": [
    "cdk/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "cdktf.out"
  ]
}
